{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "A food delivery service app",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "build": "babel server -d dist",
    "start-dev": "npm run build && nodemon dist/app.js",
    "test": "nyc --reporter=html --reporter=text-lcov _mocha ./tests --recursive --require babel-register --exit --timeout 5000",
    "test-dev": " nyc _mocha ./tests --recursive --exit --timeout 5000",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Beautblessing/Fast-Food-Fast.git"
  },
  "keywords": [
    "fast",
    "food",
    "tasty",
    "food",
    "excellent",
    "fast",
    "food",
    "delivery",
    "services"
  ],
  "author": "Blessing Jerry",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Beautblessing/Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/Beautblessing/Fast-Food-Fast#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.18.3",
    "eslint": "^5.6.0",
    "express": "^4.16.3",
    "flowtype": "^2.0.0",
    "morgan": "^1.9.1",
    "standard": "^12.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4",
    "nyc": "^13.0.1",
    "request": "^2.88.0",
    "supertest": "^3.3.0"
  }
}
